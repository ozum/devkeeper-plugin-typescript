{
  "name": "devkeeper-plugin-typescript",
  "version": "0.0.1",
  "description": "Devkeeper TypeScript plugin.",
  "keywords": [
    "devkeeper",
    "plugin"
  ],
  "engines": {
    "node": ">=12.18.0"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/lib/**/!(*.spec|*.test)*.*",
    "/dist/**/!(*.spec|*.test)*.*",
    "/@types",
    "/module-files",
    ".eslintrc.js",
    "husky.config.js",
    "jest.config.js",
    "lint-staged.config.js",
    "prettier.config.js",
    "tsconfig.json"
  ],
  "homepage": "https: //github.com/ozum/devkeeper-plugin-typescript",
  "bugs": "https: //github.com/ozum/devkeeper-plugin-typescript/issues",
  "repository": "github:ozum/devkeeper-plugin-typescript",
  "author": {
    "name": "Özüm Eldoğan"
  },
  "license": "MIT",
  "scripts": {
    "file:watch": "ts-node-dev --no-notify --respawn --transpileOnly --compiler-options '{ \"module\": \"commonjs\" }'",
    "format": "devkeeper format",
    "lint": "devkeeper lint",
    "readme": "devkeeper readme",
    "release": "devkeeper release",
    "test": "devkeeper test --coverage false --testTimeout 10000 --testPathIgnorePatterns '(src/)|(dist/)'"
  },
  "dependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "commitizen": "^4.1.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "lodash.union": "^4.6.0",
    "prettier": "^2.0.5",
    "readmeasy": "^0.2.7",
    "spdx-license-list": "^6.2.0",
    "ts-jest": "^26.1.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/lodash.union": "^4.6.6",
    "@types/tmp": "^0.2.0",
    "@types/yargs": "^15.0.5",
    "cli-docs-generator": "^1.0.3",
    "devkeeper": "^1.0.0",
    "intermodular": "^0.34.0",
    "yargs": "^15.4.1"
  }
}
